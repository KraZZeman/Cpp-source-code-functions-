#include <iostream>
#include <time.h>
#include <cstdlib>

using namespace std;

int main()
{
    int how_many= ;
    int from = ;
    int to = ;
    int result[how_many];

    if(to-from<how_many)
    {
        cout <<"error: to many results in relation to range of randomization! Press any key to exit..."<<endl;
        getchar();
        exit(0);
    }
    srand(time(NULL));

    for(int i=0; i<how_many; i++)
    {
        result[i]=(rand()%(to-from)+1)+from;
        for(int j=0; j<i; j++)
        {
            if(result[j]==result[i])
            {
                i--;
                break;
            }
        }
    }
    for(int i=0; i<how_many; i++)
        cout << result[i]<< endl;

return 0;
}
